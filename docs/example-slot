#!/bin/ash
# shellcheck shell=dash disable=1091
#Import libraries
. formating
slotnumber=$(basename "$0" | sed 's/slot\-//')
if [ ! -f "/etc/musicbox/slots/slot$slotnumber.conf" ]; then
    error "configuration for slot $slotnumber is missing, it is pertinent creating it!! Using default for now"
fi
get-unplayed-songs
